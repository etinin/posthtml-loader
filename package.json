{
  "name": "posthtml-loader",
  "description": "PostHTML for Webpack",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "standard",
    "clean": "rm -rf jest coverage jsdoc-api dmd test/builds",
    "pretest": "node test/index.js",
    "test": "jest --verbose",
    "posttest": "npm run clean",
    "docs": "jsdoc2md index.js > INDEX.md",
    "release": "standard-version"
  },
  "dependencies": {
    "ajv": "^4.11.3",
    "loader-utils": "^1.0.2",
    "posthtml": "^0.9.2",
    "posthtml-load-config": "^1.0.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.16",
    "fs-extra": "^2.0.0",
    "jest": "^19.0.2",
    "jsdoc-to-markdown": "^3.0.0",
    "posthtml-expressions": "^1.1.0",
    "posthtml-sugarml": "1.0.0-alpha3",
    "standard": "^9.0.1",
    "standard-version": "^4.0.0",
    "webpack": "^2.2.1"
  },
  "standard": {
    "env": {
      "jest": true
    },
    "ignore": [
      "*.test.js"
    ]
  },
  "files": [
    "lib",
    "index.js"
  ],
  "keywords": [
    "html",
    "posthtml",
    "webpack",
    "webpack-loader"
  ],
  "author": {
    "name": "Michael Ciniawky",
    "email": "michael.ciniawsky@gmail.com"
  },
  "contributors": [
    {
      "name": "Jeff Escalante",
      "email": "gh.je@mailhero.io"
    },
    {
      "name": "Ivan Demidov",
      "email": "Scrum@list.ru",
      "url": "https://twitter.com/Scrum_"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/posthtml/posthtml-loader.git"
  },
  "bugs": {
    "url": "https://github.com/posthtml/posthtml-loader/issues"
  },
  "homepage": "https://github.com/posthtml/posthtml-loader",
  "license": "MIT"
}
